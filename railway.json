{
  "services": [
    {
      "name": "chatgpt-voting-api",
      "source": {
        "type": "git"
      },
      "build": {
        "builder": "NIXPACKS"
      },
      "variables": {
        "DATABASE_URL": "${{postgres.DATABASE_URL}}",
        "JWT_ISSUER": "",
        "JWT_AUDIENCE": "",
        "JWKS_URL": "",
        "ADMIN_SECRET": ""
      },
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port $PORT",
      "healthcheck": {
        "path": "/health"
      }
    },
    {
      "name": "postgres",
      "plugin": "postgresql",
      "template": "railwayapp-templates/postgres-ssl:17"
    }
  ]
}
